{
  tree_schema: 'ykp/{env}/{oem}',
  source: {
    root_path: 'dist',
  },
  target: {
    id: 'alicloud',
    type: 'alicloud',
    bucket: '',
    access_key: '${ak}',
    access_secret: '${as}',
    root_path: '/',
  },
  endpoints: {
    'ykp/test/oem1': {
      build: 'yarn build:oem1',
      defines: {
        NGINX_REPLACE_B: 'http://',
        NGINX_REPLACE_A: '替换了',
      },
      domain: 'ci-test.rys.com',
    },
    'ykp/test/oem2': {
      // build: 'yarn build:test:oem2',
      build: 'yarn build:oem2',
      defines: {
        NGINX_REPLACE_B: 'http://',
        NGINX_REPLACE_A: '替换了',
      },
      domain: 'ci-test.rys.com'
    },
  },
  groups: [
    {
      name: '%group1',
      level: 'oem',
      includes: [
        'rys',
        'zhs',
      ],
    },
    {
      name: '%group1',
      level: 'oem',
      includes: [
        'zhs',
        'cy',
      ],
    }
  ],
}
